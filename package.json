{
  "name": "standalonerequestserver",
  "private": true,
  "version": "1.0.0",
  "description": "Standalone basic single-venue request server implementation for use with OpenKJ.",
  "type": "module",
  "scripts": {
    "updatelocalassets": "cp node_modules/@exampledev/new.css/new.css css/new.css && cp node_modules/htmx.org/dist/htmx.min.js js/htmx.min.js",
    "buildcss": "NODE_ENV=production node_modules/.bin/cleancss -O 2 -o css/style.min.css node_modules/@exampledev/new.css/new.css css/venuestyle.css",
    "watchcss": "node_modules/.bin/cleancss --watch -O 2 -o css/style.min.css node_modules/@exampledev/new.css/new.css css/venuestyle.css",
    "buildjs": "NODE_ENV=production node_modules/.bin/uglifyjs -o js/script.min.js --compress --mangle -- node_modules/htmx.org/dist/htmx.min.js js/script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apiontek/StandaloneRequestServer.git"
  },
  "keywords": [
    "karaoke",
    "songbook",
    "search"
  ],
  "author": "OpenKJ, Adam Piontek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apiontek/StandaloneRequestServer/issues"
  },
  "homepage": "https://github.com/apiontek/StandaloneRequestServer#readme",
  "devDependencies": {
    "@exampledev/new.css": "^1.1.3",
    "clean-css-cli": "^5.6.1",
    "htmx.org": "^1.8.4",
    "uglify-js": "^3.17.4"
  }
}
